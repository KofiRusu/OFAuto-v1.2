{
  "name": "ofauto",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3015",
    "build": "next build --no-lint",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "validate-env": "node scripts/validate-env.js",
    "setup-db": "./scripts/setup-local-db.sh",
    "test-runner": "node scripts/test-runner.js",
    "db:reset": "cp .env.local .env && npx prisma migrate reset --force",
    "db:migrate": "cp .env.local .env && npx prisma migrate dev",
    "db:seed": "cp .env.local .env && npx prisma db seed",
    "db:generate": "cp .env.local .env && npx prisma generate",
    "db:init": "./scripts/init-db.sh",
    "db:fix": "./scripts/fix-db-connection.sh",
    "start-localhost": "./start-localhost.sh",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:a11y": "start-server-and-test storybook 6006 'node scripts/a11y-test.js'",
    "test:lighthouse": "lhci autorun",
    "test:smoke": "node scripts/smoke-test.js",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy:ui": "chmod +x ./scripts/deploy-ui.sh && ./scripts/deploy-ui.sh",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "analyze": "ANALYZE=true next build",
    "load-test": "k6 run load-tests/api-load-test.js"
  },
